<?xml version="1.0" encoding="utf-8"?>
<data-map xmlns="http://cayenne.apache.org/schema/10/modelMap"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://cayenne.apache.org/schema/10/modelMap https://cayenne.apache.org/schema/10/modelMap.xsd"
	 project-version="10">
	<property name="defaultPackage" value="ule.chat.model"/>
	<db-entity name="degrees" catalog="ule_chat">
		<db-attribute name="id" type="BIGINT" isPrimaryKey="true" isGenerated="true" isMandatory="true" length="19"/>
		<db-attribute name="name" type="VARCHAR" isMandatory="true" length="255"/>
	</db-entity>
	<db-entity name="files" catalog="ule_chat">
		<db-attribute name="date_uploaded" type="TIMESTAMP" isMandatory="true" length="19"/>
		<db-attribute name="id" type="BIGINT" isPrimaryKey="true" isGenerated="true" isMandatory="true" length="19"/>
		<db-attribute name="metadata" type="VARCHAR" length="700"/>
		<db-attribute name="name" type="VARCHAR" isMandatory="true" length="255"/>
		<db-attribute name="path" type="VARCHAR" length="400"/>
		<db-attribute name="size" type="INTEGER" isMandatory="true" length="10"/>
		<db-attribute name="type" type="VARCHAR" isMandatory="true" length="10"/>
		<db-attribute name="user_id" type="BIGINT" isMandatory="true" length="19"/>
	</db-entity>
	<db-entity name="sessions" catalog="ule_chat">
		<db-attribute name="date_started" type="TIMESTAMP" isMandatory="true" length="19"/>
		<db-attribute name="id" type="BIGINT" isPrimaryKey="true" isGenerated="true" isMandatory="true" length="19"/>
		<db-attribute name="src_ip" type="VARCHAR" isMandatory="true" length="16"/>
		<db-attribute name="token" type="VARCHAR" isMandatory="true" length="128"/>
		<db-attribute name="user_id" type="BIGINT" isMandatory="true" length="19"/>
	</db-entity>
	<db-entity name="stu_subj" catalog="ule_chat">
		<db-attribute name="student_id" type="BIGINT" isPrimaryKey="true" isMandatory="true" length="19"/>
		<db-attribute name="subject_id" type="BIGINT" isPrimaryKey="true" isMandatory="true" length="19"/>
	</db-entity>
	<db-entity name="subjects" catalog="ule_chat">
		<db-attribute name="credits" type="TINYINT" isMandatory="true" length="3"/>
		<db-attribute name="degree_id" type="BIGINT" isMandatory="true" length="19"/>
		<db-attribute name="grade" type="TINYINT" isMandatory="true" length="3"/>
		<db-attribute name="id" type="BIGINT" isPrimaryKey="true" isGenerated="true" isMandatory="true" length="19"/>
		<db-attribute name="name" type="VARCHAR" isMandatory="true" length="30"/>
		<db-attribute name="subj_period" type="VARCHAR" isMandatory="true" length="15"/>
		<db-attribute name="type" type="VARCHAR" isMandatory="true" length="50"/>
	</db-entity>
	<db-entity name="tea_subj" catalog="ule_chat">
		<db-attribute name="subject_id" type="BIGINT" isPrimaryKey="true" isMandatory="true" length="19"/>
		<db-attribute name="teacher_id" type="BIGINT" isPrimaryKey="true" isMandatory="true" length="19"/>
	</db-entity>
	<db-entity name="users" catalog="ule_chat">
		<db-attribute name="age" type="TINYINT" length="3"/>
		<db-attribute name="birthdate" type="DATE" length="10"/>
		<db-attribute name="block_until" type="TIMESTAMP" length="19"/>
		<db-attribute name="email" type="VARCHAR" isMandatory="true" length="70"/>
		<db-attribute name="full_name" type="VARCHAR" isMandatory="true" length="100"/>
		<db-attribute name="id" type="BIGINT" isPrimaryKey="true" isGenerated="true" isMandatory="true" length="19"/>
		<db-attribute name="password" type="BINARY" isMandatory="true" length="60"/>
		<db-attribute name="role" type="VARCHAR" isMandatory="true" length="13"/>
		<db-attribute name="username" type="VARCHAR" isMandatory="true" length="20"/>
	</db-entity>
	<obj-entity name="Degrees" className="ule.chat.model.Degrees" dbEntityName="degrees">
		<obj-attribute name="name" type="java.lang.String" db-attribute-path="name"/>
	</obj-entity>
	<obj-entity name="Files" className="ule.chat.model.Files" dbEntityName="files">
		<obj-attribute name="dateUploaded" type="java.time.LocalDateTime" db-attribute-path="date_uploaded"/>
		<obj-attribute name="metadata" type="java.lang.String" db-attribute-path="metadata"/>
		<obj-attribute name="name" type="java.lang.String" db-attribute-path="name"/>
		<obj-attribute name="path" type="java.lang.String" db-attribute-path="path"/>
		<obj-attribute name="size" type="int" db-attribute-path="size"/>
		<obj-attribute name="type" type="java.lang.String" db-attribute-path="type"/>
	</obj-entity>
	<obj-entity name="Sessions" className="ule.chat.model.Sessions" dbEntityName="sessions">
		<obj-attribute name="dateStarted" type="java.time.LocalDateTime" db-attribute-path="date_started"/>
		<obj-attribute name="srcIp" type="java.lang.String" db-attribute-path="src_ip"/>
		<obj-attribute name="token" type="java.lang.String" db-attribute-path="token"/>
	</obj-entity>
	<obj-entity name="Subjects" className="ule.chat.model.Subjects" dbEntityName="subjects">
		<obj-attribute name="credits" type="short" db-attribute-path="credits"/>
		<obj-attribute name="grade" type="short" db-attribute-path="grade"/>
		<obj-attribute name="name" type="java.lang.String" db-attribute-path="name"/>
		<obj-attribute name="subjPeriod" type="java.lang.String" db-attribute-path="subj_period"/>
		<obj-attribute name="type" type="java.lang.String" db-attribute-path="type"/>
	</obj-entity>
	<obj-entity name="Users" className="ule.chat.model.Users" dbEntityName="users">
		<obj-attribute name="age" type="short" db-attribute-path="age"/>
		<obj-attribute name="birthdate" type="java.time.LocalDate" db-attribute-path="birthdate"/>
		<obj-attribute name="blockUntil" type="java.time.LocalDateTime" db-attribute-path="block_until"/>
		<obj-attribute name="email" type="java.lang.String" db-attribute-path="email"/>
		<obj-attribute name="fullName" type="java.lang.String" db-attribute-path="full_name"/>
		<obj-attribute name="password" type="byte[]" db-attribute-path="password"/>
		<obj-attribute name="role" type="java.lang.String" db-attribute-path="role"/>
		<obj-attribute name="username" type="java.lang.String" db-attribute-path="username"/>
	</obj-entity>
	<db-relationship name="subjects" source="degrees" target="subjects" toMany="true">
		<db-attribute-pair source="id" target="degree_id"/>
	</db-relationship>
	<db-relationship name="user" source="files" target="users">
		<db-attribute-pair source="user_id" target="id"/>
	</db-relationship>
	<db-relationship name="user" source="sessions" target="users">
		<db-attribute-pair source="user_id" target="id"/>
	</db-relationship>
	<db-relationship name="student" source="stu_subj" target="users">
		<db-attribute-pair source="student_id" target="id"/>
	</db-relationship>
	<db-relationship name="subject" source="stu_subj" target="subjects">
		<db-attribute-pair source="subject_id" target="id"/>
	</db-relationship>
	<db-relationship name="degree" source="subjects" target="degrees">
		<db-attribute-pair source="degree_id" target="id"/>
	</db-relationship>
	<db-relationship name="stuSubjs" source="subjects" target="stu_subj" toDependentPK="true" toMany="true">
		<db-attribute-pair source="id" target="subject_id"/>
	</db-relationship>
	<db-relationship name="teaSubjs" source="subjects" target="tea_subj" toDependentPK="true" toMany="true">
		<db-attribute-pair source="id" target="subject_id"/>
	</db-relationship>
	<db-relationship name="subject" source="tea_subj" target="subjects">
		<db-attribute-pair source="subject_id" target="id"/>
	</db-relationship>
	<db-relationship name="teacher" source="tea_subj" target="users">
		<db-attribute-pair source="teacher_id" target="id"/>
	</db-relationship>
	<db-relationship name="files" source="users" target="files" toMany="true">
		<db-attribute-pair source="id" target="user_id"/>
	</db-relationship>
	<db-relationship name="sessions" source="users" target="sessions" toMany="true">
		<db-attribute-pair source="id" target="user_id"/>
	</db-relationship>
	<db-relationship name="stuSubjs" source="users" target="stu_subj" toDependentPK="true" toMany="true">
		<db-attribute-pair source="id" target="student_id"/>
	</db-relationship>
	<db-relationship name="teaSubjs" source="users" target="tea_subj" toDependentPK="true" toMany="true">
		<db-attribute-pair source="id" target="teacher_id"/>
	</db-relationship>
	<obj-relationship name="subjects" source="Degrees" target="Subjects" deleteRule="Deny" db-relationship-path="subjects"/>
	<obj-relationship name="user" source="Files" target="Users" deleteRule="Nullify" db-relationship-path="user"/>
	<obj-relationship name="user" source="Sessions" target="Users" deleteRule="Nullify" db-relationship-path="user"/>
	<obj-relationship name="degree" source="Subjects" target="Degrees" deleteRule="Nullify" db-relationship-path="degree"/>
	<obj-relationship name="students" source="Subjects" target="Users" db-relationship-path="stuSubjs.student"/>
	<obj-relationship name="teachers" source="Subjects" target="Users" db-relationship-path="teaSubjs.teacher"/>
	<obj-relationship name="files" source="Users" target="Files" deleteRule="Deny" db-relationship-path="files"/>
	<obj-relationship name="sessions" source="Users" target="Sessions" deleteRule="Deny" db-relationship-path="sessions"/>
	<obj-relationship name="subjectsOfStudent" source="Users" target="Subjects" db-relationship-path="stuSubjs.subject"/>
	<obj-relationship name="subjectsOfTeacher" source="Users" target="Subjects" db-relationship-path="teaSubjs.subject"/>
	<dbImport xmlns="http://cayenne.apache.org/schema/10/dbimport">
		<catalog>
			<name>ule_chat</name>
		</catalog>
		<tableTypes>
			<tableType>TABLE</tableType>
			<tableType>VIEW</tableType>
		</tableTypes>
		<forceDataMapCatalog>false</forceDataMapCatalog>
		<forceDataMapSchema>false</forceDataMapSchema>
		<namingStrategy>org.apache.cayenne.dbsync.naming.DefaultObjectNameGenerator</namingStrategy>
		<skipPrimaryKeyLoading>false</skipPrimaryKeyLoading>
		<skipRelationshipsLoading>false</skipRelationshipsLoading>
		<useJava7Types>false</useJava7Types>
		<usePrimitives>true</usePrimitives>
	</dbImport>
	<cgen xmlns="http://cayenne.apache.org/schema/10/cgen">
		<destDir>../java</destDir>
		<mode>entity</mode>
		<template>templates/v4_1/subclass.vm</template>
		<superTemplate>templates/v4_1/superclass.vm</superTemplate>
		<template>templates/v4_1/subclass.vm</template>
		<superTemplate>templates/v4_1/superclass.vm</superTemplate>
		<embeddableTemplate>templates/v4_1/embeddable-subclass.vm</embeddableTemplate>
		<embeddableSuperTemplate>templates/v4_1/embeddable-superclass.vm</embeddableSuperTemplate>
		<queryTemplate>templates/v4_1/datamap-subclass.vm</queryTemplate>
		<querySuperTemplate>templates/v4_1/datamap-superclass.vm</querySuperTemplate>
		<outputPattern>*.java</outputPattern>
		<makePairs>true</makePairs>
		<usePkgPath>true</usePkgPath>
		<overwrite>false</overwrite>
		<createPropertyNames>false</createPropertyNames>
		<createPKProperties>false</createPKProperties>
		<client>false</client>
	</cgen>
</data-map>
